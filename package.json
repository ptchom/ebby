{
  "name": "ebby",
  "private": true,
  "author": "andron13",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:ignore": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --no-ignore",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format:fix": "prettier --write \"./**/*.{json,cjs,js,jsx,ts,tsx,css,md}\"",
    "typecheck": "tsc",
    "ncu:check": "ncu",
    "ncu:update": "ncu --interactive",
    "pre-commit": "lint-staged",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test --config=./app/__tests__/playwright.config.ts",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@react-pdf/renderer": "^3.4.4",
    "@remix-run/node": "^2.12.0",
    "@remix-run/react": "^2.12.0",
    "@remix-run/serve": "^2.12.0",
    "@tailwindcss/forms": "^0.5.9",
    "clsx": "^2.1.1",
    "firebase": "^10.13.1",
    "flowbite-react": "^0.10.1",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.26.2",
    "react-select": "^5.8.0",
    "remix-i18next": "^6.3.0",
    "remix-utils": "^7.6.0",
    "tailwindcss-hero-patterns": "^0.1.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.1",
    "@playwright/test": "^1.47.1",
    "@remix-run/dev": "^2.12.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.11",
    "typescript": "5.5.4",
    "vite": "^5.4.6",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format:fix"
    ],
    "**/*.{json,css,scss,md}": [
      "npm run format:fix"
    ]
  }
}
